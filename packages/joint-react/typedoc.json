{
  "entryPoints": ["src/index.ts"],
  "outputs": [
    {
      "name": "markdown",
      "path": "./docs"
    }
  ],
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-mdn-links"],
  "readme": "none",
  "name": "@joint/react",
