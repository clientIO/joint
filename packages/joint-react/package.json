{
  "name": "@joint/react",
  "version": "1.0.0-alpha.5",
  "type": "module",
  "description": "React bindings and hooks for JointJS to build interactive diagrams and graphs.",
  "license": "MPL-2.0",
  "keywords": [
    "jointjs",
    "react",
    "diagramming",
    "graph",
    "flowchart",
    "uml",
    "visualization",
    "editor",
    "drawing",
    "stencil"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "src"
  ],
  "homepage": "https://jointjs.com",
  "author": {
    "name": "client IO",
    "url": "https://client.io"
  },
  "contributors": [
    "Roman Bruckner <roman@jointjs.com> (https://github.com/kumilingus)",
    "Samuel <samuel@jointjs.com> (https://github.com/samuelgja)"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "yarn eslint \"**/*.{ts,tsx}\"",
    "format": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "test": "yarn run typecheck && yarn run lint && yarn jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "node --experimental-json-modules --loader ts-node/esm build.ts",
    "docs:typedoc": "typedoc --out docs/api src",
    "prepublishOnly": "echo \"Publishing via NPM is not allowed!\" && exit 1",
    "prepack": "yarn test && yarn build",
    "knip": "knip"
  },
  "devDependencies": {
    "@andypf/json-viewer": "^2.1.10",
